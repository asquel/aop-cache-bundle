{
    "name": "AsQuel/aop-cache-bundle",
    "type": "symfony-bundle",
    "description": "Simply add a @Cacheable annotation for every method you like",
    "repositories": [
    ],
    "require": {
        "php": ">=5.3.3",
        "goaop/goaop-symfony-bundle": "~2.0",
        "symfony/framework-bundle": "~2.7"
    },
    "require-dev": {
        "doctrine/doctrine-cache-bundle": "~1.3",
        "tedivm/stash-bundle": "0.4.*",
        "psr/cache": "~1.0",
        "symfony/browser-kit": "~2.4",
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "AsQuel\\AopCacheBundle\\": "src"
        }
    }
}